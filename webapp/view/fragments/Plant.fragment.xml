<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>SelectPlant}"
		class="sapUiPopupWithPadding"
		items="{PlantModel>/results}"
        search="_plantGroupValueHelpSearch"
		confirm="_plantGroupValueHelpClose"
		cancel="_plantGroupValueHelpClose">
		<StandardListItem title="{PlantModel>PRODPLANT}" info="{PlantModel>DESCRIPTION}"/>
	</SelectDialog>
</core:FragmentDefinition>