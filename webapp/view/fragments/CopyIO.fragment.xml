                        <f:formElements>
                            <f:FormElement label="{ddtext>/NEWSTYLE}">
                                <f:fields>
                                    <CheckBox id="StyleCB" selected="true" />
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{ddtext>/COLOR}">
                                <f:fields>
