                            <f:FormElement label="{ddtext>/BOM}">
                                <f:fields>
                                    <CheckBox id="BOMCB" selected="true" />
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{ddtext>/COSTING}">
                                <f:fields>
                                    <CheckBox id="CostingCB" selected="true" />
                                </f:fields>
                            </f:FormElement>
                        </f:formElements>
                        <f:layoutData>
                            <l:GridData span="XL5 L5 M5 S5" />
