<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>SelectSalesTerm}"
		class="sapUiPopupWithPadding"
		items="{SalesTermModel>/results}"
        search="_salestermGroupValueHelpSearch"
		confirm="_salestermGroupValueHelpClose"
		cancel="_salestermGroupValueHelpClose">
		<StandardListItem title="{SalesTermModel>FTYSALTERM}" info="{SalesTermModel>DESCRIPTION}"/>
	</SelectDialog>
</core:FragmentDefinition>