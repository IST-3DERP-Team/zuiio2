<core:FragmentDefinition
		xmlns="sap.m"
        xmlns:f="sap.ui.layout.form"
		xmlns:core="sap.ui.core"
        xmlns:uit="sap.ui.table"
        fragmentName="fragment1">
    <Dialog contentWidth="1000px"
        contentHeight="300px"
        title="DC Sending Detail"
        resizable="true"
        draggable="true">
        <endButton>
			<Button text="Close" press=".onFADCSendDetailClose"/>
		</endButton>
        <uit:Table id="faDCSendDetailTab"
            rows="{/items}"
            selectionMode="MultiToggle"
            visibleRowCount="{/rowCount}"
            >
        </uit:Table>    
    </Dialog>
</core:FragmentDefinition>
