<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>SelectCustGrp}"
		class="sapUiPopupWithPadding"
		items="{CustGrpModel>/results}"
        search="_custgrpGroupValueHelpSearch"
		confirm="_custgrpGroupValueHelpClose"
		cancel="_custgrpGroupValueHelpClose">
		<StandardListItem title="{CustGrpModel>CUSTGRP}" info="{CustGrpModel>DESCRIPTION}"/>
	</SelectDialog>
</core:FragmentDefinition>