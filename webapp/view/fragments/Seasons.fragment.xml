		title="{i18n>SelectSeason}"
		class="sapUiPopupWithPadding"
		items="{SeasonsModel>/results}"
        search="_seasonsGroupValueHelpSearch"
		confirm="_seasonsGroupValueHelpClose"
		cancel="_seasonsGroupValueHelpClose">
		<StandardListItem title="{SeasonsModel>SEASONCD}" info="{SeasonsModel>DESC1}"/>
	</SelectDialog>
</core:FragmentDefinition>