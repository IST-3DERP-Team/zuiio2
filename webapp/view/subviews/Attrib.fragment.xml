                    <FlexItemData growFactor="1" baseSize="0%"/>
                </c:layoutData>  
            </c:DynamicTable> -->

            <t:Table id="AttribDynTable" 
                class="sapUiSizeCompact sidePadding" 
                visibleRowCountMode="Auto"
                showColumnVisibilityMenu="true" git 
