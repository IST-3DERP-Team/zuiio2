<core:FragmentDefinition 
    xmlns="sap.m" 
    xmlns:l="sap.ui.layout" 
    xmlns:f="sap.ui.layout.form" 
    xmlns:core="sap.ui.core" 
    xmlns:mvc="sap.ui.core.mvc"
    class="sapUiSizeCompact" 
    xmlns:tnt="sap.tnt"
    xmlns:c="zuiio2.control" 
    xmlns:t="sap.ui.table"> 

    <!-- IO HEADER BUTTONS -->
    <OverflowToolbar id="_ovfToolBarIO" design="Transparent" >
        <Button id = "onIOEdit" type="Ghost"  icon="sap-icon://edit" tooltip="Edit IO" press="onIOEdit('IOHDR')" visible="true"/>
        <Button id = "onIORelease" type="Ghost"  icon="sap-icon://open-command-field" text="Release IO" tooltip="Release IO" press="onIORelease('IOHDR')" visible="true"/>
        <Button id = "onIOSave" type="Ghost"  icon="sap-icon://save" text="Save" tooltip="Release IO" press="onIOSave('IOHDR')" visible="false"/>
        <Button id = "onIOCancel" type="Ghost"  icon="sap-icon://sys-cancel" text="Cancel" tooltip="Release IO" press="onIOCancel('IOHDR')" visible="false"/>
    </OverflowToolbar>

    <MessageStrip id="HeaderMessageStrip" text="" showIcon="true" showCloseButton="true" class="sapUiMediumMarginBottom" type="Error" visible="false"/>
    
    <!-- FORM -->
    <f:Form id="IOHeaderForm1" editable="false">

        <!-- FORM LAYOUT -->
        <f:layout>
            <f:ResponsiveGridLayout columnsXL="4" columnsL="2" columnsM="1" singleContainerFullSize="false" />
        </f:layout>

        <!-- FORM CONTAINERS -->
        <f:formContainers>

            <!-- GROUP 1 FORM CONTAINER -->
            <f:FormContainer ariaLabelledBy="Grp1" class="sapUiNoContentPadding" >
                <f:formElements>
                    <f:FormElement label="{i18n>IONO}" visible="{= ${VisibleFieldsData>/IONO} === true ? true : false }">
                        <f:fields>
                            <Input id="IONO" value="{headerData>/IONO}" editable="false" valueStateText="Required">
                            </Input>
                        </f:fields>
                    </f:FormElement>  
                    <f:FormElement label="{i18n>STYLECD}" visible="{= ${VisibleFieldsData>/STYLECD} === true ? true : false }">
                        <f:fields>
                            <Input id="STYLECD" value="{headerData>/STYLECD}" showSuggestion="true" showValueHelp="true" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>   
                    <f:FormElement label="{i18n>PRODTYPE}" visible="{= ${VisibleFieldsData>/PRODTYPE} === true ? true : false }">
                        <f:fields>
                            <Input id="PRODTYPE" value="{headerData>/PRODTYPE}" showSuggestion="true" showValueHelp="true" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>PRODSCEN}" visible="{= ${VisibleFieldsData>/PRODSCEN} === true ? true : false }">
                        <f:fields>
                            <Input id="PRODSCEN" value="{headerData>/PRODSCEN}" showSuggestion="true" showValueHelp="true" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>SALESORG}" visible="{= ${VisibleFieldsData>/SALESORG} === true ? true : false }">
                        <f:fields>
                            <Input id="SALESORG" value="{headerData>/SALESORG}" showSuggestion="true" showValueHelp="true" editable="false" valueStateText="Required"
                                change="onHeaderChange" liveChange="onHeaderChange" >
                            </Input>
                        </f:fields>
                    </f:FormElement>  
                    <f:FormElement label="{i18n>ORDQTY}" visible="{= ${VisibleFieldsData>/ORDQTY} === true ? true : false }">
                        <f:fields>
                            <Input id="ORDQTY" type="Number" value="{headerData>/ORDQTY}" editable="{HeaderEditModeModel>/editMode}"
                                change="onHeaderChange" liveChange="onHeaderChange" >
                            </Input>
                        </f:fields>
                    </f:FormElement>  
                    <f:FormElement label="{i18n>ACTUALQTY}" visible="{= ${VisibleFieldsData>/ACTUALQTY} === true ? true : false }">
                        <f:fields>
                            <Input id="ACTUALQTY" type="Number" value="{headerData>/ACTUALQTY}" editable="{HeaderEditModeModel>/editMode}"
                                change="onHeaderChange" liveChange="onHeaderChange" >
                            </Input>
                        </f:fields>
                    </f:FormElement>  
                    <f:FormElement label="{i18n>PLANMONTH}" visible="{= ${VisibleFieldsData>/PLANMONTH} === true ? true : false }">
                        <f:fields>
                            <Input id="PLANMONTH" value="{headerData>/PLANMONTH}" showSuggestion="true" showValueHelp="true" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"
                                change="onHeaderChange" liveChange="onHeaderChange" >
                            </Input>
                        </f:fields>
                    </f:FormElement>                                                                                                                             
                </f:formElements>
            </f:FormContainer> 

            <!-- GROUP 2 FORM CONTAINER -->
            <f:FormContainer ariaLabelledBy="Grp2">
                <f:formElements>                                                            
                    <f:FormElement label="{i18n>IOTYPE}" visible="{= ${VisibleFieldsData>/IOTYPE} === true ? true : false }">
                        <f:fields>
                            <Input id="IOTYPE"  value="{headerData>/IOTYPE}" showSuggestion="true" showValueHelp="true" valueHelpRequest="handleValueHelp" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"
                                change="onHeaderChange" liveChange="onHeaderChange">                                        
                            </Input>
                        </f:fields>
                    </f:FormElement>    
                    <f:FormElement label="{i18n>SALESGRP}" visible="{= ${VisibleFieldsData>/SALESGRP} === true ? true : false }">
                        <f:fields>
                            <Input id="SALESGRP" value="{headerData>/SALESGRP}" showSuggestion="true" showValueHelp="true" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>PRODPLANT}" visible="{= ${VisibleFieldsData>/PRODPLANT} === true ? true : false }">
                        <f:fields>
                            <Input id="PRODPLANT" value="{headerData>/PRODPLANT}" showSuggestion="true" showValueHelp="true" editable="false" valueStateText="Required"
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>FTYSALESTERM}" visible="{= ${VisibleFieldsData>/FTYSALESTERM} === true ? true : false }">
                        <f:fields>
                            <Input id="FTYSALESTERM" value="{headerData>/FTYSALESTERM}" showSuggestion="true" showValueHelp="true" editable="false" valueStateText="Required"
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement> 
                    <f:FormElement label="{i18n>REVORDQTY}" visible="{= ${VisibleFieldsData>/REVORDQTY} === true ? true : false }">
                        <f:fields>
                            <Input id="REVORDQTY" type="Number" value="{headerData>/REVORDQTY}" editable="{HeaderEditModeModel>/editMode}"
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>  
                    <f:FormElement label="{i18n>SHIPQTY}" visible="{= ${VisibleFieldsData>/SHIPQTY} === true ? true : false }">
                        <f:fields>
                            <Input id="SHIPQTY" type="Number" value="{headerData>/SHIPQTY}" editable="{HeaderEditModeModel>/editMode}"
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>       
                    <f:FormElement label="{i18n>PRODWK}" visible="{= ${VisibleFieldsData>/PRODWK} === true ? true : false }">
                        <f:fields>
                            <Input id="PRODWK" type="Number" value="{headerData>/PRODWK}" showSuggestion="true" showValueHelp="true" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>                                                                                                         
                </f:formElements>
            </f:FormContainer>  
                     
            <!-- GROUP 3 FORM CONTAINER -->
            <f:FormContainer ariaLabelledBy="Grp3">
                <f:formElements>                            
                    <f:FormElement label="{i18n>IOSUFFIX}" visible="{= ${VisibleFieldsData>/IOSUFFIX} === true ? true : false }">
                        <f:fields>
                            <Input id="IOSUFFIX" type="Text" showSuggestion="true" showValueHelp="true" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"
                                value="{headerData>/IOSUFFIX}" 
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>SEASONCD}" visible="{= ${VisibleFieldsData>/SEASONCD} === true ? true : false }">
                        <f:fields>
                            <Input id="SEASONCD" type="Text" showSuggestion="true" showValueHelp="true" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"
                                value="{headerData>/SEASONCD}" 
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>CUSTGRP}" visible="{= ${VisibleFieldsData>/CUSTGRP} === true ? true : false }">
                        <f:fields>
                            <Input id="CUSTGRP" type="Text" showSuggestion="true" showValueHelp="true" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"
                                value="{headerData>/CUSTGRP}" 
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement> 
                    <f:FormElement label="{i18n>TRADPLANT}" visible="{= ${VisibleFieldsData>/TRADPLANT} === true ? true : false }">
                        <f:fields>
                            <Input id="TRADPLANT" type="Text" showSuggestion="true" showValueHelp="true" editable="false" valueStateText="Required"
                                value="{headerData>/TRADPLANT}" 
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>    
                    <f:FormElement label="{i18n>CUSSALETERM}" visible="{= ${VisibleFieldsData>/CUSSALETERM} === true ? true : false }">
                        <f:fields>
                            <Input id="CUSSALETERM" type="Text" showSuggestion="true" showValueHelp="true" editable="false" valueStateText="Required"
                                value="{headerData>/CUSSALETERM}" 
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>BASEUOM}" visible="{= ${VisibleFieldsData>/BASEUOM} === true ? true : false }">
                        <f:fields>
                            <Input id="BASEUOM" type="Text" showSuggestion="true" showValueHelp="true" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"
                                value="{headerData>/BASEUOM}" 
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>PLANDLVDT}" visible="{= ${VisibleFieldsData>/PLANDLVDT} === true ? true : false }">
                        <f:fields>
                            <DateTimePicker
                                id="PLANDLVDT"
                                placeholder="Enter Date"
                                value="{headerData>/PLANDLVDT}" 
                                editable="{HeaderEditModeModel>/editMode}"
                                change="handleChange"
                                class="sapUiSmallMarginBottom"
                                valueFormat="yyyy-MM-dd-HH-mm-ss" displayFormat="short"/>
                            <!-- <Input id="PLANDLVDT" type="Text" showSuggestion="true" showValueHelp="true" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"
                                value="{headerData>/PLANDLVDT}" 
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input> -->
                        </f:fields>
                    </f:FormElement>                                                                                                                                            
                </f:formElements>
            </f:FormContainer> 

            <!-- GROUP 4 FORM CONTAINER-->
            <f:FormContainer ariaLabelledBy="Grp4">
                <f:formElements>                            
                    <f:FormElement label="{i18n>REFIONO}" visible="{= ${VisibleFieldsData>/REFIONO} === true ? true : false }">
                        <f:fields>
                            <Input id="REFIONO" type="Text" showSuggestion="true" showValueHelp="true" editable="false" valueStateText="Required"
                                value="{headerData>/REFIONO}" 
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>   
                    <f:FormElement label="{i18n>STYLENO}" visible="{= ${VisibleFieldsData>/STYLENO} === true ? true : false }">
                        <f:fields>
                            <Input id="STYLENO" type="Text" showSuggestion="true" showValueHelp="true" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"
                                value="{headerData>/STYLENO}" 
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>VERNO}" visible="{= ${VisibleFieldsData>/VERNO} === true ? true : false }">
                        <f:fields>
                            <Input id="VERNO"  type="Number" editable="false"
                                value="{headerData>/VERNO}" 
                                change="onHeaderChange" liveChange="onHeaderChange">
                            </Input>
                        </f:fields>
                    </f:FormElement>                                                                                 
                    <f:FormElement label="{i18n>PLANPLANT}" visible="{= ${VisibleFieldsData>/PLANPLANT} === true ? true : false }">
                        <f:fields>
                            <Input id="PLANPLANT" value="{headerData>/PLANPLANT}" showSuggestion="true" showValueHelp="true" editable="false" valueStateText="Required"/>
                        </f:fields>
                    </f:FormElement> 
                    <f:FormElement label="{i18n>CUSTDLVDT}"  visible="{= ${VisibleFieldsData>/CUSTDLVDT} === true ? true : false }">
                        <f:fields>
                            <DateTimePicker
                                id="CUSTDLVDT"
                                placeholder="Enter Date"
                                change="handleChange"
                                editable="{HeaderEditModeModel>/editMode}"
                                value="{headerData>/CUSTDLVDT}" 
                                class="sapUiSmallMarginBottom"
                                valueFormat="yyyy-MM-dd-HH-mm-ss" displayFormat="short"/>
                            <!-- <Input id="CUSTDLVDT"  value="{headerData>/CUSTDLVDT}" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"/> -->
                        </f:fields>
                    </f:FormElement> 
                    <f:FormElement label="{i18n>PLANQTY}"  visible="{= ${VisibleFieldsData>/PLANQTY} === true ? true : false }">
                        <f:fields>
                            <Input id="PLANQTY" type="Number" value="{headerData>/PLANQTY}" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"/>
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>PRODSTART}"  visible="{= ${VisibleFieldsData>/PRODSTART} === true ? true : false }">
                        <f:fields>
                            <DateTimePicker
                                id="PRODSTART"
                                placeholder="Enter Date"
                                change="handleChange"
                                editable="{HeaderEditModeModel>/editMode}"
                                class="sapUiSmallMarginBottom"
                                valueFormat="yyyy-MM-dd-HH-mm-ss" displayFormat="short"/>
                            <!-- <Input id="PRODSTART" value="{headerData>/PRODSTART}" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"/> -->
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>REMARKS}"  visible="{= ${VisibleFieldsData>/REMARKS} === true ? true : false }">
                        <f:fields>
                            <Input id="REMARKS" value="{headerData>/REMARKS}" editable="{HeaderEditModeModel>/editMode}" valueStateText="Required"/>
                        </f:fields>
                    </f:FormElement>                            
                </f:formElements>
            </f:FormContainer>

        </f:formContainers>

    </f:Form>  
</core:FragmentDefinition>